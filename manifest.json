{
  "manifest_version": 3,
  "name": "图文解锁器",
  "version": "1.0",
  "description": "一键解除网页复制/右键/选中限制，支持截图与自选区域 OCR 识别",
  "permissions": ["activeTab", "storage", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
